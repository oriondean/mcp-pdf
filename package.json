{
  "name": "pdf-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for fetching and parsing PDFs from URLs with text extraction and image rendering support",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "mcp",
    "pdf",
    "parser",
    "server"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@napi-rs/canvas": "^0.1.58",
    "node-fetch": "^3.3.2",
    "pdfjs-dist": "^4.8.69",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.19.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.15"
  }
}
